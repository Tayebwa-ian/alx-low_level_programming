#include <stdio.h>
/**
 * print_times_table - printing time table
 *
 * @n: table of integer n to be built
 *
 * Return: void or 0 if n is less 0 or greater 15
 */
void print_times_table(int n)
{
if (n < 0 || n > 15)
{
return (0);
}
else
{
int i, j, result;
for (i = 0; i < n; i++)
{
for (j = 0; j < n; j++)
{
result = i * j;
if (j != n)
{
printf("%d, ", result);
}
else
{
printf("%d", result);
}
}
printf("\n");
}
}
}
