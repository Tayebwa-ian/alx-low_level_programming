#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/* more headers goes there */

/**
 * main - program main entry
 *
 * Description: "Last digit thing",
 *
 * Return: 0 (Success)
*/
int main(void)
{
  int n, m;

srand(time(0));
n = rand() - RAND_MAX / 2;
/* your code goes there */
m = n%10;
if (m > 5)
{
printf("Last digit of %d is %d and is greater than 5\n", n, m)
}
return (0);
}
