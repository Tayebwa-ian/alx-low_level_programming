#include <stdio.h>
#include <stdlib.h>
/**
 *main -  multplies two numbers
 *@argc: number of arguments provided
 *@argv: the arguments
 *Return: 0
 */
int main(int argc, char *argv[])
{
	if (argc != 3)
	{
		printf("Error");
		return (0);
	}
	printf("%d\n", atoi(argv[1]) * atoi(argv[2]));
	return (0);
}
