#include <stdio.h>
/**
 * main - Entry point
 *
 * Description: 'C program that prints the size of various types'
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
printf("Size of a char: %d byte(s)",sizeof(char));
 printf("Size of an int: %d byte(s)",sizeof(int));
printf("Size of a char: %d byte(s)",sizeof(char));
printf("Size of a char: %d byte(s)",sizeof(char));
return (0);
}
